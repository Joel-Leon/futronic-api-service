{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "FutronicService": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Fingerprint": {
    "Threshold": 70,
    "Timeout": 30000,
    "TempPath": "C:/temp/fingerprints",
    "CapturePath": "C:/temp/fingerprints/captures",
    "StoragePath": "C:/SistemaHuellas/huellas",
    "OverwriteExisting": false,
    "MaxTemplatesPerIdentify": 500,
    "MaxRotation": 199,
    "DeviceCheckOnStartup": true,
    "DeviceCheckRetries": 3,
    "DeviceCheckDelayMs": 1000
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:3001",
      "http://localhost:3002",
      "http://127.0.0.1:3000",
      "http://127.0.0.1:3001",
      "http://127.0.0.1:3002"
    ]
  }
}
